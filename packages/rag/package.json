{
  "name": "@seashore/rag",
  "version": "0.1.0",
  "description": "RAG (Retrieval-Augmented Generation) for seashore agents",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@seashore/vectordb": "workspace:*",
    "@seashore/llm": "workspace:*",
    "pdf-parse": "^1.1.1",
    "node-html-parser": "^6.1.12"
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "rollup": "^4.40.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "vitest": "^3.1.1"
  },
  "peerDependencies": {
    "zod": "^4.0.5"
  },
  "keywords": [
    "rag",
    "retrieval",
    "augmented",
    "generation",
    "ai",
    "llm",
    "embeddings"
  ],
  "author": "Seashore Team",
  "license": "MIT"
}