{
  "name": "@seashore/workflow",
  "version": "0.1.0",
  "description": "Workflow orchestration for Seashore Agent Framework",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
