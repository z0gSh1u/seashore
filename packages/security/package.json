{
  "name": "@seashore/security",
  "version": "0.1.0",
  "description": "Security guardrails and content filtering for Seashore framework",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@seashore/llm": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "rollup": "^4.9.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "seashore",
    "security",
    "guardrails",
    "agent",
    "llm",
    "content-filter"
  ],
  "license": "MIT"
}